<div class="col-xs-12 col-sm-4 col-md-3">
  <div class="panel panel-default ">
    <div class="panel-heading list-name", onclick="window.location.href = '<%= url_for list_path(list) %>'"><%= list.name %><%= link_to list, method: :delete, data: { confirm: 'Are you sure?' } do %>
          <span class='glyphicon glyphicon-trash'></span>
        <% end %></div>
    <table class="table">
      <tr onclick="window.location.href = '<%= url_for location_path(list) %>'">
      </tr>
        <% list.locations.each do |l| %>
          <tr onclick="window.location.href = '<%= url_for l %>'">
            <td class='list-item'><%= l.name %></td>
          </tr>
        <% end %>
    </table>
  </div>
</div>